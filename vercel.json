{
  "builds": [
    {         
      "src": "index.js",
      "use": "@vercel/node@16.7.0"
    }
  ],
"routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "dest": "/api/$1",
        "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
  
}
